//include includes/dashboard.pug
dashboard
.row
  h2 Upload CSV Data
  fieldset
    legend Upload CSV File
    form(action='/uploadfile', method='post', enctype='multipart/form-data')
      input#Collection(name='Collection', type='hidden', value='SampleData')
        
      label(for='upload') Choose file:
        
      input(type='file', accept='.csv', name='uploadfile')
        
      button(type='submit', name='submit') Submit
.row
  #composite-chart.u6.chart-wrapper
    .chart-title.bold
      strong Media Volume
  #data-table.u6.chart-wrapper
    .chart-title.bold
      strong Media Support
    table#sampledatatable.rwd-table(border='1')
      thead
        tr
          th Month
          th Sales Vol.
          th TV
          th Radio
          th Newspaper
.row
  #model-table.u6.chart-wrapper
    .chart-title.bold
      strong Model
    table#mix-model-summary-console.console-data
      thead
        tr
          th(colspan='4') OLS Regression Result
      tbody
        tr
          td Dep. Variable
          td(data-attr-data-name='dep_var')
          td R-Squared
          td(data-attr-data-name='rsquared')
        tr
          td Models
          td(data-attr-data-name='model')
          td Adj. R-Squared
          td(data-attr-data-name='rsquared_adj')
        tr
          td Method
          td(data-attr-data-name='method')
          td PROB (F-Statics)
          td(data-attr-data-name='prob_f_statistic')
        tr
          td DATE
          td(data-attr-data-name='datetime', data-value='date')
          td R-Squared
          td(data-attr-data-name='rsquared')
        tr
          td TIME
          td(data-attr-data-name='datetime', data-value='time')
          td Log-Likelihood
          td(data-attr-data-name='log_likelihood')
        tr
          td No. Observations
          td(data-attr-data-name='no_of_observations')
          td AIC
          td(data-attr-data-name='aic')
        tr
          td DF. Residuals
          td(data-attr-data-name='df_resid')
          td BIC
          td(data-attr-data-name='bic')
        tr
          td DF. Model
          td(data-attr-data-name='dep_var')
          td
          td(data-attr-data-name='')
        tr
          td Converiance Type
          td(data-attr-data-name='cov_type')
          td
          td(data-attr-data-name='')
      tfoot
        tr
          th
          th
          th
          th
    table#mix-model-console1.console-data
      tbody
        tr
          td Inercept
          td(data-attr-data-name='dep_var')
          td(data-attr-data-name='dep_var')
          td(data-attr-data-name='dep_var')
          td(data-attr-data-name='dep_var')
          td(data-attr-data-name='dep_var')
          td(data-attr-data-name='dep_var')
        tr
          td TV
          td(data-attr-data-name='dep_var')
          td(data-attr-data-name='dep_var')
